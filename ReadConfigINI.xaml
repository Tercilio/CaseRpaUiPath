    <Assign DisplayName="Obtendo todos os valores dentro do config" sap:VirtualizedContainerService.HintSize="450,116" sap2010:WorkflowViewState.IdRef="Assign_1">
      <Assign.To>
        <OutArgument x:TypeArguments="scg:Dictionary(x:String, x:String)">[GlobalVariablesNamespace.GlobalVariables.configDict]</OutArgument>
      </Assign.To>
      <Assign.Value>
        <InArgument x:TypeArguments="scg:Dictionary(x:String, x:String)" xml:space="preserve">[(From line In configContent.Split({Environment.NewLine}, StringSplitOptions.RemoveEmptyEntries)
    Let l = line.Trim()
    Where Not String.IsNullOrWhiteSpace(l) AndAlso Not l.StartsWith(";") AndAlso l.Contains("=") AndAlso Not l.StartsWith("[")
    Let partes = l.Split({"="}, 2, StringSplitOptions.None)
    Select partes
).ToDictionary(Function(p) p(0).Trim(), Function(p) p(1).Trim())]</InArgument>
      </Assign.Value>
    </Assign>
